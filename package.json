{
    "name": "myde",
    "version": "0.0.1",
    "description": "my desktop",
    "main": "./out/main/main.js",
    "scripts": {
        "pack": "npm run build && electron-builder --dir -c electron-builder.config.js",
        "dist": "npm run build && electron-builder -p never -c electron-builder.config.js",
        "start": "electron-vite --ignoreConfigWarning preview",
        "dev": "electron-vite --ignoreConfigWarning dev",
        "build": "electron-vite --ignoreConfigWarning build",
        "pkgRebuild": "electron-rebuild",
        "desktop": "export desktop='desktop' && electron-vite --ignoreConfigWarning preview"
    },
    "author": {
        "name": "xsf",
        "email": "xushengfeng_zg@163.com"
    },
    "homepage": "https://github.com/xushengfeng/myde/",
    "license": "AGPL-3.0-only",
    "dependencies": {
        "@xushengfeng/usocket": "^1.1.1",
        "dkh-ui": "^0.14.2",
        "ini": "^5.0.0",
        "xml2js": "^0.6.2"
    },
    "devDependencies": {
        "@biomejs/biome": "^2.2.4",
        "@electron/rebuild": "^4.0.1",
        "@types/ini": "^4.1.1",
        "@types/xml2js": "^0.4.14",
        "electron": "^31.4.0",
        "electron-builder": "^24.4.0",
        "electron-vite": "^4.0.1",
        "typescript": "^5.9.2",
        "vite": "^7.1.7"
    }
}
